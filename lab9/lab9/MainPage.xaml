<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Lab9.Views"
             x:Class="Lab9.Views.MainPage"
             Title="Main">
    <VerticalStackLayout Padding="20">
        <Button Text="Manage Models" Command="{Binding NavigateToModelsCommand}" />
        <Button Text="Manage Mechanics" Command="{Binding NavigateToMechanicsCommand}" />
        <Button Text="About" Command="{Binding NavigateToAboutCommand}" />
        <Button Text="Logout" Command="{Binding LogoutCommand}" />
    </VerticalStackLayout>
</ContentPage>
