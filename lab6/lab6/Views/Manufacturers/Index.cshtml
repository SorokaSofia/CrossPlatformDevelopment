@model IEnumerable<Lab6.Data.Manufacturer>

<h2>Manufacturers</h2>
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ManufacturerName</td>
                <td><a href="@Url.Action("Details", new { id = item.Id })">View</a></td>
            </tr>
        }
    </tbody>
</table>
